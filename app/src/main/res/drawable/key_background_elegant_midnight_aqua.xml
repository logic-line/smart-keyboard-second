<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1dp"
        android:end="1dp"
        android:start="1dp"
        android:top="1dp">
        <shape>
            <padding
                android:bottom="0.5dp"
                android:left="0.5dp"
                android:right="0.5dp"
                android:top="0.5dp" />
            <stroke
                android:width="1dp"
                android:color="@color/elegant_midnight_aqua_key_bg_color" />
            <solid android:color="@color/elegant_midnight_aqua_key_bg_color" />
            <corners android:radius="2dp" />
        </shape>
    </item>
</layer-list>